{
  "project": "HelpX Internal",
  "host": "helpx-internal.adobe.com",
  "plugins": [
    {
      "id": "tools",
      "title": "Tools",
      "isContainer": true,
      "environments": [
        "edit"
      ],
      "includePaths": [
        "**.docx**"
      ]
    },
    {
      "containerId": "tools",
      "id": "localize-2",
      "title": "Localize (V2)",
      "environments": [
        "edit"
      ],
      "url": "https://main--helpx-internal--adobecom.hlx.live/tools/loc?milolibs=locui",
      "passReferrer": true,
      "passConfig": true,
      "includePaths": [
        "**.xlsx**"
      ]
    },
    {
      "containerId": "tools",
      "id": "milo-library",
      "title": "Milo Library",
      "environments": [
        "edit"
      ],
      "isPalette": true,
      "passConfig": true,
      "paletteRect": "top: auto; bottom: 20px; left: 20px; height: 398px; width: 360px;",
      "url": "https://milo.adobe.com/tools/library",
      "includePaths": [
        "**.docx**"
      ]
    },
    {
      "id": "helpx-library",
      "containerId": "tools",
      "title": "Helpx Library",
      "environments": [
        "edit"
      ],
      "isPalette": true,
      "passConfig": true,
      "paletteRect": "top: auto; bottom: 20px; left: 20px; height: 398px; width: 360px;",
      "url": "https://main--helpx-internal--adobecom.hlx.page/tools/library",
      "includePaths": [
        "**.docx**"
      ]
    },
    {
      "title": "Preflight",
      "id": "preflight",
      "environments": [
        "dev",
        "preview",
        "live"
      ],
      "event": "preflight"
    }
  ]
}
